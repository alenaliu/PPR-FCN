train_net: "models/sg_vrd/rel_pre_iccv/train_pre_psroi_context_tri_sum_deep.prototxt"
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize:20000 
display: 20

momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot:500
# We still use the snapshot prefix, though
snapshot_prefix: "output/rel_iccv/pre_psroi_context_tri_sum_deep"
iter_size: 2
# debug_info: true
