train_net: "models/sg_vrd/rel_iccv/train_classeme_gt_cached.prototxt"
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
stepsize: 8000000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot:500
# We still use the snapshot prefix, though
snapshot_prefix: "output/sg_vrd_rfcn/classeme_gt_cached"
iter_size: 2
# debug_info: true
