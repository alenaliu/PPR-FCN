train_net: "models/sg_vrd/relation/train.prototxt"
base_lr: 0.1
lr_policy: "step"
gamma: 0.1
stepsize: 30000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 500
# We still use the snapshot prefix, though
snapshot_prefix: "output/relation/sg_vrd_relation_vgg16"
iter_size: 2
