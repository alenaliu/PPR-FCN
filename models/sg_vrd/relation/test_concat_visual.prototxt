name: "VGG_ILSVRC_16_layers"

input: "visual"
input_shape {
  dim: 1
  dim: 8192
}

layer {
  name: "relation"
  type: "InnerProduct"
  bottom: "visual"
  top: "relation"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 70
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}


layer {
  name: "relation_prob"
  type: "Softmax"
  bottom: "relation"
  top: "relation_prob"
}
